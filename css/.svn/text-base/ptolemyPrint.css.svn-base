	}

	#walking-panel {
		width: 100%;
		display: block;
		overflow: : visible;
	}
	#walking-directionsl {
		width: 100%;
		display: block;
		overflow: : visible;
	}
	#control {
		width:0;
		height:0;
	}
	#map-canvas {
		display: none;
	}
	#my-schedule {
		display: none;
	}
}
